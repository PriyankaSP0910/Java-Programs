1)select e.fname,e.lname,d.dname from employee e join department d on e.did=d.did where e.salary>(select avg(e1.salary) from employee e1 join department d1 on e1.did=d1.did where d1.dname='Finance')and d.dname!='finance';

2)select e.fname,e.lname,d.dname from employee e join department d on e.did=d.did join works_on w on w.ssn=e.ssn join project p on p.pid=w.pid where d.dname='R&D' and p.did=d.did group by e.fname,e.lname,d.dname having count(w.pid)>2;

3) select p.pname from project p join department d on d.did=p.did where p.status='ongoing';

4)select e.ssn,e.fname,e.lname,e.address,e.salary from employee e join employee e1 on e1.sup_ssn=e.ssn join works_on w on w.ssn=e.ssn join project p on p.pid=w.pid where p.status='completed' group by e.ssn,e.fname,e.lname,e.address,e.salary having count(e1.sup_ssn)>3 and count(distinct w.pid)>0;

5)select d.relationship from dependent d join employee e on d.ssn=e.ssn join works_on w on e.ssn=w.ssn join project p on p.pid=w.pid group by d.relationship having sum(p.worth)>=1000000;

6) select d.dname,e.fname from employee e join department d on d.did=e.did join project p on p.did=d.did join project_loc pl on p.pid=pl.pid group by d.dname,e.fname having count(distinct location)>1;
